using System;

public class Program
{
    public void Main()
    {
        Console.WriteLine("Enter the number of rows for the pyramid:");
        int rows = int.Parse(Console.ReadLine());

        for (int i = 1; i <= rows; i++)
        {
            for (int j = 1; j <= i; j++)
            {
                Console.Write(i);
            }
            Console.WriteLine();
        }
    }
}
